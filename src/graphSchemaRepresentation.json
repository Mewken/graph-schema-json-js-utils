{
    "graphSchemaRepresentation": {
      "version": "1.0.1",
      "graphSchema": {
        "version": "1.0.0",
        "nodeLabels": [ //finns nodeLabels? - K
          {
            "$id": "l1", //finns id - K 
            "token": "Person"//finns token - K
            //tillåt inga extra fält? - K
          },
          {
            "$id": "l2",
            "token": "Actor"
          },
          {
            "$id": "l3",
            "token": "Director"
          },
          {
            "$id": "l4",
            "token": "Movie"
          }
        ],
        "relationshipTypes": [ //finns rel.type? - K
          {
            "$id": "rt1", //Finns id - K
            "token": "ACTED_IN"//finns token - K
            //tillåts inga extra fält? - K
          },
          {
            "$id": "rt2",
            "token": "DIRECTED"
          }
        ],
        "nodeSpecs": [//Finns Nodespecs? - K
          {
            "$id": "n1",//Finns id? - K
            "labels": [//finns label - K
            //Tillåts inga extra fält? - K
              {
                "$ref": "#l1"//Finns ref.
                //Tillåts inga extra fält? - K
              },
              {
                "$ref": "#l2"
              }
            ],
            "properties": [
              {
                "token": "name",//Finns token? - K
                "type": "string"//Finns type? - K
                //Tillåts inga extra fält - K
              },
              {
                "token": "born",
                "type": "integer"
              }
            ]
          },
          {
            "$id": "n2",
            "labels": [
              {
                "$ref": "#l1"
              },
              {
                "$ref": "#l3"
              }
            ],
            "properties": [
              {
                "token": "name",
                "type": "string"
              },
              {
                "token": "born",
                "type": "integer"
              }
            ]
          },
          {
            "$id": "n3",
            "labels": [
              {
                "$ref": "#l4"
              }
            ],
            "properties": [
              {
                "token": "title",
                "type": "string"
              },
              {
                "token": "release",
                "type": "date"
              }
            ]
          }
        ],
        "relationshipSpecs": [//Finns rel.specs - K
          {
            "$id": "r1",//Finns id? - K
            "type": {//finns type
              "$ref": "#rt1"
              //Tillåts inga extra fält? -K
            },
            //Tillåts inga extra fält? -K
            "from": {
              "$ref": "#n1"
            },
            "to": {
              "$ref": "#n3"
            },
            "properties": [//finns properties-K
              {
                "token": "role",
                "type": "array",
                //Tillåts inga extra fält? - K
                "items": { //finns item om typ är array
                  "type": "string"
                }
              }
            ]
          },
          {
            "$id": "r2",
            "type": {
              "$ref": "#rt2"
            },
            "from": {
              "$ref": "#n2"
            },
            "to": {
              "$ref": "#n3"
            },
            "properties": [
              {
                "token": "role",//finns token-K
                "type": "string" //finns type-K
                //Tillåts inga extra fält?-K
              }
            ]
          }
        ]
      }
    }
  }